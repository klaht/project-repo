      <Button fx:id="setTextButton" defaultButton="true" layoutX="243.0" layoutY="23.0" mnemonicParsing="false" onAction="#setButtonText" text="Save task" />
      <Button layoutX="243.0" layoutY="56.0" mnemonicParsing="false" onAction="#popupDeleteTask" prefHeight="25.0" prefWidth="66.0" text="Delete" />
